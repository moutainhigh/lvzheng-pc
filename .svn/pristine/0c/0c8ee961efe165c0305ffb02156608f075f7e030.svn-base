@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}

.suit-box{background: #f5fdfe;padding-bottom: 80px}
.suit-banner{width: 100%; min-width:1200px; height: 520px;background: #17A7FE url('http://static.lvzheng.com/lvzhengpc/images/suit/suit_banner.jpg')  no-repeat center;}
.suit-banner .banner-box{width: 1200px; height: 520px; margin: 0 auto;position: relative;}
.suit-banner .banner-box a.tel{display: block; font-size: 20px;color: #ff4545;height: 30px;line-height: 30px;position: absolute;top: 30px;left: 100px;font-style: italic;}
.suit-main{width: 1100px;  height: 1550px; margin: 0 auto;background:  url('http://static.lvzheng.com/lvzhengpc/images/suit/mainbg.png')  no-repeat center; margin-top: -40px; position: relative;}

.suit-main .box1{width: 1000px; margin: 0 auto; height: 590px; position: relative;overflow: hidden;}
.suit-main .box1 .go-tag{position: absolute;width: 80px;height: 110px; top: 10px; left: 690px; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/go_ico.png')  no-repeat center;}
.suit-main .box1 .b-tag{position: absolute;width: 180px;height: 180px; top: 100px; left: 150px; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/b_tag1.png')  no-repeat center;}
.suit-main .box1 .b-tit{width: 190px;height: 70px; margin: 200px auto 0; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/b_tit1.png')  no-repeat center;}

.suit-main .box1 .cp-box{width: 630px;height: 65px;margin: 30px auto}
.suit-main .box1 .cp-box ul{overflow: hidden;}
.suit-main .box1 .cp-box ul li{float: left;height: 65px;line-height: 65px;font-size: 20px;text-align: center;font-weight: bold;}
.suit-main .box1 .cp-box ul li.cp1{width: 170px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/cp_box1.png')  no-repeat center;}
.suit-main .box1 .cp-box ul li.cp2{width: 200px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/cp_box2.png')  no-repeat center;}
.suit-main .box1 .cp-box ul li.plus{width: 42px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/plus_ico.png')  no-repeat center;}

.suit-main .price-box{width: 295px;height: 55px; margin: 0 auto; border-left: 1px solid #cccccc;border-right: 1px solid #cccccc}
.suit-main .price-box ul{overflow: hidden;}
.suit-main .price-box ul li{width: 146px;height: 55px; border-right: 1px solid #ccc;float: left;text-align: center;}
.suit-main .price-box ul li:last-child{border-right: none}
.suit-main .price-box ul li .area{font-size: 18px;color: #000;height: 20px;line-height: 20px}
.suit-main .price-box ul li .price{font-size: 30px;color: #ff0000;height: 35px;line-height: 35px;font-weight: bold;}

.suit-main .buy-box{width: 500px;min-height: 65px; margin: 40px auto 20px;}
.suit-main .buy-box .text{width: 270px;float: left;text-align: right;}
.suit-main .buy-box .text .t1{font-size: 24px;color: #000;font-weight: bold;height: 35px;line-height: 35px}
.suit-main .buy-box .text .t2{font-size: 16px;height: 30px;line-height: 30px}
.suit-main .buy-box a.buybtn{ display: block;float: left; margin-left: 20px; width: 180px;text-align: center;height: 65px;line-height: 60px;font-size: 27px;color: #fff;font-weight: bold;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/btnbg.png')  no-repeat center;}

.suit-main .box2{width: 1000px; margin: 0 auto; height: 440px; position: relative;overflow: hidden;}
.suit-main .box2 .b-tag{position: absolute;width: 200px;height: 160px; top: 25px; right: 70px; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/b_tag2.png')  no-repeat center;}
.suit-main .box2 .b-tit{width: 190px;height: 70px; margin: 50px auto 0; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/b_tit2.png')  no-repeat center;}
.suit-main .box2 .cp-box{width: 390px;height: 65px;margin: 30px auto}
.suit-main .box2 .cp-box ul{overflow: hidden;}
.suit-main .box2 .cp-box ul li{float: left;height: 65px;line-height: 65px;font-size: 20px;text-align: center;font-weight: bold;}
.suit-main .box2 .cp-box ul li.cp1{width: 170px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/cp_box1.png')  no-repeat center;}
.suit-main .box2 .cp-box ul li.cp2{width: 200px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/cp_box2.png')  no-repeat center;}
.suit-main .box2 .cp-box ul li.plus{width: 42px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/plus_ico.png')  no-repeat center;}

.suit-main .box3{width: 1000px; margin: 0 auto; padding-bottom: 130px; height: 440px; position: relative;overflow: hidden;}
.suit-main .box3 .b-tag{position: absolute;width: 260px;height: 180px; top: 15px; left: 50px; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/b_tag3.png')  no-repeat center;}
.suit-main .box3 .b-tit{width: 190px;height: 70px; margin: 50px auto 0; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/b_tit3.png')  no-repeat center;}
.suit-main .box3 .cp-box{width: 390px;height: 65px;margin: 30px auto}
.suit-main .box3 .cp-box ul{overflow: hidden;}
.suit-main .box3 .cp-box ul li{float: left;height: 65px;line-height: 65px;font-size: 20px;text-align: center;font-weight: bold;}
.suit-main .box3 .cp-box ul li.cp1{width: 170px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/cp_box1.png')  no-repeat center;}
.suit-main .box3 .cp-box ul li.cp2{width: 200px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/cp_box2.png')  no-repeat center;}
.suit-main .box3 .cp-box ul li.plus{width: 42px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/plus_ico.png')  no-repeat center;}

.bot-info{width: 100%;min-height: 160px;padding: 40px 0;background: #fffaee}
.bot-info .info-box{width: 1100px;margin: 0 auto;min-height: 160px;}
.bot-info .info-box .title{width: 370px; margin: 10px auto; text-align: center; height: 40px;line-height: 40px; background: url('http://static.lvzheng.com/lvzhengpc/images/suit/line.png')  repeat-x center;}
.bot-info .info-box .title span{display: block; width: 175px;margin: 0 auto;font-size: 30px;color: #000; background: #fffaee}
.bot-info .info-box .text{height: 30px;line-height: 30px; width: 740px;margin: 0 auto; font-size: 18px;color: #7b4c38;text-align: left;padding: 5px 0}

.bot-tel{height: 80px;line-height: 80px; width: 100%; background: #192934;display: none;}
.bot-tel .tel-box{width: 380px;margin: 0 auto;color: #fff;font-size: 30px;padding-left: 65px;background: url('http://static.lvzheng.com/lvzhengpc/images/suit/tel_ico.png')  no-repeat left center;}
.bot-telxf {position: fixed;bottom: 0px;z-index: 9000;display: block;}