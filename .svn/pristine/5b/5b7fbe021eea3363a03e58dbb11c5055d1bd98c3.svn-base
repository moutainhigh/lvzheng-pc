.kz-box .main-box .fw-box .needcl{width: 900px;margin: 20px auto;min-height:300px;overflow: hidden; }
.kz-box .main-box .fw-box .needcl ul{overflow: hidden;}
.kz-box .main-box .fw-box .needcl ul li{width: 380px;padding-left: 30px; min-height: 100px; margin: 20px 20px 40px 20px;float: left;background: url('/images/kz/listline.png')  no-repeat left top;}
.kz-box .main-box .fw-box .needcl ul li div.name{font-size: 22px; color: #000; height: 30px;line-height: 30px;padding-bottom: 10px;}
.kz-box .main-box .fw-box .needcl ul li div.text{min-height: 20px;line-height: 20px;font-size: 14px;color: #4e5d73;padding: 3px 0}
.kz-box .main-box .fw-box .needcl ul li div.text label{display: inline-block; width: 20px; height: 20px;line-height: 20px; float: left;}
.kz-box .main-box .fw-box .needcl ul li div.text p{width: 330px; min-height: 20px;line-height: 20px; float: left;padding-right: 20px}

.kz-box .main-box .fw-box .wyz-box{width: 1000px;height: 330px;padding: 30px 0}
.kz-box .main-box .fw-box .wyz-box ul{overflow: hidden;}
.kz-box .main-box .fw-box .wyz-box ul li{width: 300px;height: 30px; padding: 275px 10px 15px 10px;margin-right: 20px;float: left;position: relative;}
.kz-box .main-box .fw-box .wyz-box ul li:last-child{margin-right: 0px}
.kz-box .main-box .fw-box .wyz-box ul li.list1{background: #f4f4f4 url('/images/kz/yz1.jpg')  no-repeat center top 10px;}
.kz-box .main-box .fw-box .wyz-box ul li.list1:hover{background: #587186 url('/images/kz/yz1.jpg')  no-repeat center top 10px;}
.kz-box .main-box .fw-box .wyz-box ul li.list2{background: #f4f4f4 url('/images/kz/yz2.jpg')  no-repeat center top 10px;}
.kz-box .main-box .fw-box .wyz-box ul li.list2:hover{background: #587186 url('/images/kz/yz1.jpg')  no-repeat center top 10px;}
.kz-box .main-box .fw-box .wyz-box ul li.list3{background: #f4f4f4 url('/images/kz/yz3.jpg')  no-repeat center top 10px;}
.kz-box .main-box .fw-box .wyz-box ul li.list3:hover{background: #587186 url('/images/kz/yz1.jpg')  no-repeat center top 10px;}
.kz-box .main-box .fw-box .wyz-box ul li .yzname{width: 300px;height: 50px;line-height: 50px;font-size: 24px;color: #000;text-align: center;border-bottom: 1px solid #dbdbdb;position: absolute;top: 210px}
.kz-box .main-box .fw-box .wyz-box ul li:hover .yzname{color: #fff}
.kz-box .main-box .fw-box .wyz-box ul li .price{width: 190px;font-size: 18px; height: 30px;line-height: 30px; color: #000;float: left;}
.kz-box .main-box .fw-box .wyz-box ul li:hover .price{color: #fff}
.kz-box .main-box .fw-box .wyz-box ul li .price span{color: #ef2121}
.kz-box .main-box .fw-box .wyz-box ul li:hover .price span{color: #fff}
.kz-box .main-box .fw-box .wyz-box ul li a.zxbtn{width: 100px;height: 28px;line-height: 28px;border: 1px solid #cccccc; background: #fff; font-size: 14px;color: #000;text-align: center;border-radius: 8px;display: block;float: left;}
.kz-box .main-box .fw-box .wyz-box ul li:hover a.zxbtn{background: #ff2525;color: #fff}

.bot-box{width: 100%;height: 40px; line-height: 40px; background: #182834;padding: 10px 0;display: none;}
.bot-boxxf{position: fixed;bottom: 0px;display: block;}
.bot-box .tel-box{width: 800px;margin: 0 auto}
.bot-box .tel-box .tel{font-size: 24px;color: #ec373a;padding-left: 30px;background: url('/images/kz/tel_ico.png')  no-repeat center left;float: left;}
.bot-box .tel-box .item{float: left; width: 440px; margin-left: 30px}
.bot-box .tel-box .item input.w-text{width: 270px;height: 40px; line-height: 40px; background: #fff;padding: 0 10px;border: none;float: left;font-size: 16px;color: #333}
.bot-box .tel-box .item a.btn{width: 150px;text-align: center;height: 40px;line-height: 40px;font-size: 20px;color: #fff;background: #ec373a;display: block;float: left;}


/*弹层*/
#submit-box{margin-top: 10%;width: 420px;min-height: 100px;padding: 50px 0}
#submit-box .submit{width: 260px;padding-left: 75px;margin: 0 auto;background: url('/images/kz/c_ico.png')  no-repeat center left;}
#submit-box .submit p.name{font-size: 18px;height: 40px;line-height: 40px;color: #000}
#submit-box .submit p.text{font-size: 14px;min-height: 20px;line-height: 20px;color: #666}

