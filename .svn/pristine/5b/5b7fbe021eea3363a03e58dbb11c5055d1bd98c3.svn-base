.kz-box .main-box .wkz-box{width: 1000px;margin: 0 auto;margin-top: 70px;}
.kz-box .main-box .wkz-box .buy-box{width: 1000px; height: 150px; margin: 0 auto;padding-top: 350px;background: url('/images/kz/kzbg.jpg')  no-repeat center top;}
.kz-box .main-box .wkz-box .buy-box a.kzbtn{display: block;width: 210px;text-align: center;height: 60px;line-height: 60px;font-size: 24px;color: #fff; background: #f12525;border-radius: 3px; margin: 0 auto}
