		$(".list-ul2 li").addClass("b-box").removeClass("checked");
	    $(this).addClass("checked").removeClass("b-box");
			var listData = getData("/brand/getBrandList",id);
				if(listData == null){
					return;
				}
				var zntypeArr = listData.zntype;
				$.each(zntypeArr,function(j,zn){
					var cgArr = zn.cg;
					var cgnum = zn.cgnum;
					var title = zn.title;
					
					var liHtml = '<div class="show-list">'+
									'<p class="title">第'+cgnum+'类 '+zn.title+'</p>'+
										'<ul>';
					
					$.each(cgArr,function(k,cg){
						var thArr = cg.th;
						$.each(thArr,function(n,th){
							liHtml += '<li>'+th.cgnum+' '+th.title+'</li>'
						})
					});
					
					liHtml += '</ul></div>';
					$('.brandResult').append(liHtml);
				})
	})
}

function getData(url,parentId){
	var resData = null;
	$.ajax({
		url: url,
		type: "post",
		dataType: "json",
		async: false,
		data:{
			"parentId" : parentId,
		},
		success:function(data){
			resData = data;
		},
		error:function(){
			alert("未响应");
		}
	});
	
	return resData;
}
