@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}

.hdtc-box{min-height: 1000px;width: 100%; background: #fff;padding-bottom: 80px}
.hdtc-box .banner{height: 640px;width: 100%;background: #425875 url('../images/hdtc/bannerbg.jpg')  no-repeat center;}
.hdtc-box .banner-box{width: 1000px;margin: 0 auto;position: relative;}
.hdtc-box .banner-box .btag{width: 440px;height: 100px;position: absolute;top: 0px;left: 0px;}
.hdtc-box .banner-box .btel{width: 280px;height: 45px;line-height: 45px;background: #48617a url('../images/hdtc/btel.png')  no-repeat center left 20px;border-radius: 20px;position: absolute;right: 0px;top: 40px;padding-left: 70px;font-size: 18px;color: #fff;font-weight: bold;}
.hdtc-box .banner-box .btext{width: 860px;height: 150px;position: absolute;top: 170px;left: 80px;}
.hdtc-box .banner-box .bbtn{height: 50px; line-height: 50px; position: absolute;top: 410px; left: 280px; text-align: center;}
.hdtc-box .banner-box .bbtn a.btn{display: inline-block;width: 160px;text-align: center;height: 50px;line-height: 50px;font-size: 20px; margin: 0 20px; color: #fff;font-weight: bold;background: #21aaf2 url('../images/hdtc/btnarr.png')  no-repeat center right 20px; padding-right: 10px; border-radius: 5px;}
.hdtc-box .banner-box .date-box{width: 650px;height: 40px;line-height: 40px;position: absolute;top: 540px;left: 170px;}
.hdtc-box .banner-box .date-box span.text{display: inline-block;font-size: 18px;color: #fff; height: 40px;line-height: 40px;float: left;margin: 0 5px;}
.hdtc-box .banner-box .date-box span.date{display: inline-block; width: 200px; letter-spacing: 2px;  height: 40px;line-height: 40px; text-align: center;font-size: 30px;color: #ffbb38;background: url('../images/hdtc/date.png')  no-repeat center;float: left;margin: 0 2px;}
.hdtc-box .banner-box .date-box span.time{display: inline-block; width: 175px; letter-spacing: -2px; height: 40px;line-height: 40px; text-align: center;font-size: 30px;color: #ffbb38;background: url('../images/hdtc/time.png')  no-repeat center;float: left;margin: 0 2px;}
.hdtc-box .banner-box .date-box span.time .clock i{font-style: normal;}

.hdtc-box .sbox{width: 100%;min-height: 300px;padding: 20px 0;background: #fff}
.hdtc-box .sbox .name-line{height: 40px;border-bottom: 1px solid #e5e5e5;position: relative;}
.hdtc-box .sbox .name-line p.name{font-size: 36px;color: #666;height: 40px;line-height: 40px; width: 180px;background: #fff; text-align: center; position: absolute;top: 20px;left: 50%;margin-left: -90px;}
.hdtc-box .sbox .name-line p.name span{color: #0ac552}
.hdtc-box .sbox .sflow{width: 1000px;min-height: 240px; margin:50px auto;background: url('../images/hdtc/sflow.png')  no-repeat center;position: relative;}
.hdtc-box .sbox .sflow .btn{display: inline-block; position: absolute; top: 210px; width: 75px;text-align: center;height: 25px;line-height: 25px;font-size: 14px;color: #19b6fa;border: 1px solid #00a0e9;border-radius: 5px;}
.hdtc-box .sbox .sflow .btn:hover{background: #0f8bcc;color: #fff}
.hdtc-box .sbox .sflow .btn1{left: 30px}
.hdtc-box .sbox .sflow .btn3{left: 462px}
.hdtc-box .sbox .bflow{width: 1000px;min-height: 200px; margin:50px auto 20px;background: url('../images/hdtc/bflow.png')  no-repeat center;position: relative;}
.hdtc-box .user {background: #f3f8fb}
.hdtc-box .user .name-line p.name{background: #f3f8fb}

.hdtc-box .sbox .text-box{width: 1000px; margin: 50px auto 20px; }
.hdtc-box .sbox .text-box .text{padding-left: 80px;width: 920px; padding-top: 10px; min-height: 160px; background:  url('../images/hdtc/jj.png')  no-repeat left top;}
.hdtc-box .sbox .text-box .text span{font-size: 22px;color: #000000;padding-left: 30px}
.hdtc-box .sbox .text-box .text{font-size: 16px;color: #000;line-height: 30px;}
.hdtc-box .sbox .text-box .tz-box{width: 540px;height: 150px;margin: 30px auto;background:  url('../images/hdtc/tz.png')  no-repeat center}
.hdtc-box .sbox .text-box .contact{font-size: 16px;color: #000;line-height: 30px;padding-left: 80px}

.hdtc-box .bot-box{width: 100%;height: 50px;line-height: 50px;padding:15px 0;background: #192836;display: none;}
.hdtc-box .bot{width: 1000px;margin: 0 auto;}
.hdtc-box .bot .tel-box{font-size: 30px;color: #fff;padding-left: 50px;background: url('../images/hdtc/btel.png')  no-repeat center left;width: 520px;float: left;}
.hdtc-box .bot .buy-box{width: 430px;float: left;}
.hdtc-box .bot .buy-box .sel-box{width: 310px; height: 50px; float: left;font-size: 18px;border: 1px solid #E1E2E0}
.hdtc-box .bot .buy-box .sel-box option{height: 30px;line-height: 30px; padding: 10px 5px; font-size: 18px;}
.hdtc-box .bot .buy-box .sel-box option{background: #FBFFDE}
.hdtc-box .bot .buy-box .buy-btn{display: inline-block; width: 120px;float: left;text-align: center;height: 50px;line-height: 50px;background: #21aaf2;font-size: 20px;color: #fff}
.hdtc-box .bot-boxxf {position: fixed;bottom: 0px;z-index: 9000;display: block;}


.nice-select3 {float: left;padding: 0 5px;height: 28px;position: relative; background: url(../images/xjt.png) no-repeat right center;cursor:pointer; }
.nice-select3 input {display: block;width: 100%;height: 20px;line-height: 20px;  padding: 5px 0; border: none;outline: 0;background: none;cursor:pointer;}
.nice-select3 ul {width: 100%;display: none;position: absolute;left: -1px;top: 30px;overflow: hidden;background-color: #fff;max-height: 150px;overflow-y: auto;border: 1px solid #ddd; margin-top: -2px; border-top: 0;z-index:9999999;}
.nice-select3 ul li {height: 30px;line-height: 30px;overflow: hidden;padding: 0 10px;cursor: pointer; font-size: 12px}
.nice-select3 ul li.on {background-color: #A5C2D2;}
.w_select_sss{width: 50px;}

.hdtc-box .sbox .pj-box{width: 1000px;min-height: 200px;margin: 50px auto 20px;}


