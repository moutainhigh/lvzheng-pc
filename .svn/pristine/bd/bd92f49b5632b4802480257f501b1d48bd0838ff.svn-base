<script type="text/javascript">
function sendMessage() {
	var jbPhone =  $("#phoneNum").val(); 
	var keyWord = $("#keyWord").val();
	
	if(keyWord == ''){
		alert('请输入关键字！');
		zxBtnEvent();//重新绑定事件
		return;
	}
	
	if(jbPhone == ''){
		alert('请输入手机号码！');
		zxBtnEvent();//重新绑定事件
		return;
	}
	
	var myreg = /^((1[0-9]{2})+\d{8})$/; 
	if(!myreg.test(jbPhone)){
		alert("手机号码格式错误！");
		zxBtnEvent();//重新绑定事件
		return;
	}
	
	$.ajax({
		url:"/brand/ldy/sendDingDingMsg",
		type:"post",
		dataType:"json",
		data:{'keyWord':keyWord,'phoneNum':jbPhone},
		success:function(data){
			if(data.ret == 'false'){
				//更改弹出框显示内容
				$("#msgTitle").html('您的网络可能出现了问题！');
				$("#msgContent").html('请您重新提交一次哦~');
			}else if(data.ret == 'true'){
				$("#phoneNum").val(''); 
				$("#keyWord").val('');
			}
		  	block();
		  	$(".cx-newtrade-box").css("display","none");
			zxBtnEvent();//重新绑定事件
		},
		error:function(){
			alert('未响应，请重试');
			zxBtnEvent();//重新绑定事件
		}
	});
	 
	 
}

function zxBtnEvent(){
	$(".js_tc").unbind('click').on('click',function(){
		$(this).unbind('click');
		 sendMessage();
	  });
}

$(function(){
	//咨询按钮绑定事件
	zxBtnEvent();
 
  $(".js_ne").click(function(){
    none();
    $(".cx-newtrade-box").css("display","block");
  });

 $('.input_test').bind({
    focus:function(){
    if (this.value == this.defaultValue){
    this.value="";
    }
    },
    blur:function(){
    if (this.value == ""){
    this.value = this.defaultValue;
    }
    }
  }); 

});

function block(){
    $(".fxd_popup").css("display","block");
    $(".fxd_pp_m").css("display","block");
 }
 function none(){
   $(".fxd_popup").css("display","none");
   $(".fxd_pp_m").css("display","none");
 }
</script>