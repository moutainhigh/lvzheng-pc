      		$(".cd-box1").removeClass('is-visible');
      	}
      });
});




function initZXbtn(){
	$('.zx_btn').unbind('click').on('click',function(){
		$(this).unbind('click');
		var teltext = $(this).parent().find(".zx_phone").val();
		if(teltext == ''){
			alert('请输入手机号码！');
			initZXbtn();//重新绑定事件
			return;
		}
		
		var myreg = /^((1[0-9]{2})+\d{8})$/; 
		if(!myreg.test(teltext)){
			alert("手机号码格式错误");
			initZXbtn();//重新绑定事件
			return;
		}
		
		sendMsgDingDing(teltext,'');
		
	});
	

}



//发送钉钉消息
function sendMsgDingDing(tel,msg){
$.ajax({
	url:"/ldy/sendDingDingMsg",
	type:"post",
	dataType:"json",
	data:{'phone':tel,'type':3,'msg':msg},
	success:function(data){
		if(data.ret == 'false'){
			$('.zxRetName').html("您的网络可能出现了问题！");
			$('.zxRetText').html("请您重新提交一次哦~");
		}
		$('.cd-box1-zx-tel').addClass('is-visible');
		
		//免费电话咨询
	    initZXbtn();
	},
	error:function(){
		alert('未响应');
		//免费电话咨询
	    initZXbtn();
	}
});
}


function openwindow(url,name,iWidth,iHeight)
{
var url; //转向网页的地址;
var name; //网页名称，可为空;
var iWidth; //弹出窗口的宽度;
var iHeight; //弹出窗口的高度;
//window.screen.height获得屏幕的高，window.screen.width获得屏幕的宽
var iTop = (window.screen.height-30-iHeight)/2; //获得窗口的垂直位置;
var iLeft = (window.screen.width-10-iWidth)/2; //获得窗口的水平位置;
window.open(url,name,'height='+iHeight+',,innerHeight='+iHeight+',width='+iWidth+',innerWidth='+iWidth+',top='+iTop+',left='+iLeft+',toolbar=no,menubar=no,scrollbars=auto,resizeable=no,location=no,status=no');
}
