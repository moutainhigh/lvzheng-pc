@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;outline: none;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}


.cx-trade-banner{width: 100%; min-width:1200px; height: 550px;background: #020a3f url('../images/trade/banner.jpg')  no-repeat center;}
.cx-trade-banner .banner-box{width: 1100px; height: 460px; margin: 0 auto;position: relative;}
.cx-trade-banner .banner-box div.btag1{display: block; height: 69px; width: 800px; position: absolute;top: 130px;left: 160px;background: url('../images/trade/btag1.png')  no-repeat center;}
.cx-trade-banner .banner-box div.btag2{display: block; width: 1100px; font-size: 16px;color: #fff; height: 30px; line-height: 30px; position: absolute;top: 220px; text-align: center;}
.cx-trade-banner .banner-box .search{padding: 10px 0;height: 55px;line-height: 55px;width: 660px;margin: 0 auto;position: absolute;top: 300px;left: 220px}
.cx-trade-banner .banner-box .search span.sea-item{ width: 485px; height: 53px;line-height: 53px; display: block; float: left; padding: 0 20px; background: #fff; border: 1px solid #b7c7d3; border-radius: 4px 0 0 4px;position: relative;}
.cx-trade-banner .banner-box .search span.sea-item input{width: 485px;height: 24px;line-height: 24px; padding: 12px 0; vertical-align: top;border: none;font-size: 16px;color: #666}
.cx-trade-banner .banner-box .search a.btn{width: 110px; padding-left: 20px; height: 55px;text-align: center;line-height: 55px;display: block;float: left;background: #ed2929 url(../images/product/cx/cx_ico.png) no-repeat center left;font-size: 18px;color: #fff;border-radius: 0 4px 4px 0;}


.cx-trade-main{width: 100%;}
.cx-trade-main p.boxname{font-size: 48px;color: #000;height: 60px;line-height: 60px;width: 500px;text-align: center;margin: 0 auto;padding: 15px 0;background:  url('../images/trade/nameline.png')  no-repeat center bottom;}
.cx-boxbg{width: 100%;background: #fff;height: 430px;padding: 50px 0}
.cx-boxbg .cxbox1{width: 1000px;height: 430px;margin: 0 auto}
.cx-boxbg .cxbox1 .trade-fw{width: 1000px;min-height: 250px;margin-top: 40px}
.cx-boxbg .cxbox1 .trade-fw ul{overflow: hidden;}
.cx-boxbg .cxbox1 .trade-fw ul li{float: left;margin: 10px;border: 1px solid #eaeaea;border-radius: 10px;}
.cx-boxbg .cxbox1 .trade-fw ul li:hover{background: #f0faff;border: 1px solid #81c0fb}
.cx-boxbg .cxbox1 .trade-fw ul li a{display: block;padding: 40px 10px 20px 10px;width: 200px;}
.cx-boxbg .cxbox1 .trade-fw ul li p{width: 200px;text-align: center;font-size: 18px;color: #1bb3ff;height: 30px;line-height: 30px;padding-top: 140px;text-decoration: underline;}
.cx-boxbg .cxbox1 .trade-fw ul li p.t1{background: url('../images/trade/t1_ico.png')  no-repeat center top;}
.cx-boxbg .cxbox1 .trade-fw ul li p.t2{background: url('../images/trade/t2_ico.png')  no-repeat center top;}
.cx-boxbg .cxbox1 .trade-fw ul li p.t3{background: url('../images/trade/t3_ico.png')  no-repeat center top;}
.cx-boxbg .cxbox1 .trade-fw ul li p.t4{background: url('../images/trade/t4_ico.png')  no-repeat center top;}

.cx-boxbg1{width: 100%;background: #eef7ff;height: 430px;padding: 50px 0}
.cx-boxbg1 .trade-flow{padding: 50px 0 30px; min-height: 250px;}
.cx-boxbg1 .trade-flow .flow-box{width: 800px; padding: 20px 0; height: 150px;margin: 0 auto}
.cx-boxbg1 .trade-flow img{width: 800px;height: 150px}
.cx-boxbg1 .trade-flow div.xw-flow{width: 620px; margin: 50px auto 0; height: 35px;line-height: 35px;font-size: 20px;text-align: center;color: #fff}
.cx-boxbg1 .trade-flow div.xw-flow span.xw{display: inline-block;width: 310px;background: #16bd88;float: left;}
.cx-boxbg1 .trade-flow div.xw-flow span.other{display: inline-block;width: 310px;background: #999999;float: left;}
.cx-boxbg1 .trade-flow div.xw-flow span.other a{display: inline-block; text-decoration: line-through;color: #fff}

.cx-boxbg .cxbox2{width: 1000px;height: 430px;margin: 0 auto}
.cx-boxbg .cxbox2 .trade-good{width: 1000px;margin-top: 50px}
.cx-boxbg .cxbox2 .good-box{width: 930px; padding: 20px 0; height: 200px;margin: 0 auto}
.cx-boxbg .cxbox2 img{width: 930px;height: 200px}

.cx-boxbg2{width: 100%;background: #eef7ff;min-height: 430px;padding: 50px 0}
.cx-boxbg2 .cxbox1 .apply{width: 800px;min-height:400px;padding: 50px 70px 20px;background: #fff;margin: 40px auto }
.cx-boxbg2 .cxbox1 .apply .app{width: 780px;height: 180px;padding: 10px;;background: #f4f4f4;margin-bottom: 30px;position: relative;}
.cx-boxbg2 .cxbox1 .apply .app p.name1{width: 25px;text-align: center;font-size: 22px;color: #fff;line-height: 25px;height: 160px;background: #67bfe5;padding: 20px 5px;position: absolute;left: 0px;top: 0px}
.cx-boxbg2 .cxbox1 .apply .app p.name2{width: 25px;text-align: center;font-size: 22px;color: #fff;line-height: 25px;height: 160px;background: #77d5ac;padding: 20px 5px;position: absolute;left: 0px;top: 0px}
.cx-boxbg2 .cxbox1 .apply .app p.img{text-align: center;width: 750px;padding-left: 20px}

#login-box{ overflow: hidden; width: 350px; background: #fff;margin-top: 10%}


.login .login_top .logo {float: left;margin: 5px 5px 0 10px;}
.login .login_top .register {border-left: 1px solid #e5e5e5;color: #999;float: left;font-size: 18px;height: 35px;line-height: 35px;margin: 10px;padding: 0 25px;}
.login .login_top .tel {background: rgba(0, 0, 0, 0) url("../images/login/telbg.png") no-repeat scroll 0 0;color: #000;float: right;font-size: 14px;height: 26px;line-height: 26px;margin-top: 15px;padding-left: 36px;text-align: right;}
.login .login_top .tel p {text-align: left;width: 100px;}
.login .login_conbox {width: 1000px;}
.login .login_con {background: rgba(0, 0, 0, 0) url("../images/trade/loginbg.png") no-repeat scroll 0 0;height: 450px;padding: 20px 0;}
.login .login_box {background: #fff none repeat scroll 0 0;border: 1px solid #eee;float: right;padding-bottom: 20px;width: 350px;}
.login .login_box input {color: #666;font-size: 14px;}
.login .login_box .box_top {padding: 30px 45px;}
.login .login_box .box_top p {float: left;}
.login .login_box .box_top .reg_name {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 0;color: #000;font-size: 18px;height: 25px;line-height: 25px;padding: 0 40px 10px 30px;}
.login .login_box .box_top .reg_login {color: #999;font-size: 12px;padding-top: 5px;}
.login .login_box .box_top .reg_login a {color: #228bcc;padding-left: 5px;}
.login .login_box .t_box1 {padding: 10px 0 10px 45px;}
.login .login_box input {border: 1px solid #ddd;float: left;height: 35px;padding: 0 10px;}
.login .login_box .w_box1 {width: 238px;}
.login .login_box .w_error {border: 1px solid #ff9f9f;}
.login .login_box .w_box2 {float: left;width: 140px;}
.login .login_box .t_box_btn {background: #f9f9f9 none repeat scroll 0 0;border: 1px solid #ddd;color: #333;cursor: pointer;display: block;float: left;font-size: 12px;height: 16px;line-height: 16px;margin-left: 10px;padding: 9px 0;text-align: center;width: 85px;}
.login .login_box .t_box_btn:hover {border: 1px solid #228bcc;color: #228bcc;}
.login .login_box .t_box1 .reg_btn {background: #66b5e5 none repeat scroll 0 0;border-radius: 3px;color: #fff;cursor: pointer;font-size: 18px;padding: 10px;text-align: center;width: 238px;}
.login .login_bott {border-top: 1px solid #eee;width: 100%;}
.login_bott p {color: #999;height: 60px;line-height: 60px;margin: 0 auto;text-align: center;width: 1000px;}
.login .login_box .box_top .reg_name1 {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 0;color: #000;font-size: 18px;height: 25px;line-height: 25px;padding: 0 60px 10px 30px;}
.login .login_box .box_top .reg_login1 {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 -33px;color: #66b5e5;font-size: 12px;height: 20px;line-height: 20px;padding: 3px 0 3px 25px;}
.login .login_box .box_top .reg_login1:hover {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 -55px;color: #ff7e7e;cursor: pointer;}
.login .login_box .box_top .reg_name2 {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 -78px;color: #000;font-size: 18px;height: 25px;line-height: 25px;padding: 0 45px 10px 30px;}
.login .login_box .box_top .reg_login2 {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 -108px;color: #66b5e5;font-size: 12px;height: 20px;line-height: 20px;padding: 3px 0 3px 25px;}
.login .login_box .box_top .reg_login2:hover {background: rgba(0, 0, 0, 0) url("../images/trade/reg_name.png") no-repeat scroll 0 -130px;color: #ff7e7e;cursor: pointer;}
.login .login_box .t_box2 {color: #999;line-height: 35px;padding: 0 48px;text-align: left;}
.login .login_box .t_box2 a {color: #999;}
.login .login_box .t_box2 a:hover {color: #228bcc;}
.login .login_box .t_box1 a {color: #228bcc;}
.login .login_box .t_box2 input {float: left;margin-right: 10px;}