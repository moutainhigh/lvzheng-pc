@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}

.day-box{min-height: 1000px;width: 100%; background: #7326ff;padding-bottom: 100px}
.day-box .banner{height: 600px;width: 100%;background: #fe1c42 url('http://static.lvzheng.com/lvzhengpc/images/day/bannerbg.jpg')  no-repeat center;}

.main-box2{width: 1116px;height: 740px; margin: 0 auto; overflow: hidden;position: relative;background: #ffc700}
.main-box2 .cy-box {width: 1050px;height: 700px;background: #fed62d;border: 5px solid #ffde51;margin: 20px auto;border-radius: 10px}
.main-box2 .cy-box .name{height: 60px; padding: 5px 0; width: 100%;background: url('http://static.lvzheng.com/lvzhengpc/images/day/namebg.png')  no-repeat center;text-align: center;}
.cy-list{padding: 0 70px}
.cy-list ul{overflow: hidden;}
.cy-list ul li{min-height: 100px; padding: 10px 0; border-bottom: 1px dashed #8f55f4}
.cy-list ul li:last-child{border-bottom: 0px}
.cy-list ul li p.list-name{width: 260px;height: 45px;line-height: 45px; padding: 5px; margin: 10px 0 0; font-size: 30px; font-weight: bold; color: #fff600; background: url('http://static.lvzheng.com/lvzhengpc/images/day/stepbg.png')  no-repeat center;}
.cy-list ul li p.list-name label{font-size: 32px;color: #b622c6;display: inline-block;width: 40px;height: 40px; line-height: 40px; margin-right: 10px; text-align: center;float: left;}
.cy-list ul li p.list-name span{font-size: 30px;display: inline-block;width: 190px;height: 40px; line-height: 40px; float: left;}

.cy-list ul li .list-con{height: 40px; line-height: 40px; font-weight: bold;margin:5px 50px;overflow: hidden;}
.cy-list ul li .list-con p.t1{font-size: 18px;color: #6a3c17;float: left;}
.cy-list ul li .list-con p.t2{font-size: 30px;color: #fd1c41;float: left;margin: 0 10px}
.cy-list ul li .list-con p.t2 span{font-size: 18px}
.cy-list ul li .list-con a.buy-btn{width: 55px;text-align: center;height: 20px;line-height: 20px;font-size: 10px;color: #fff9e1;background: #f52354;border: 1px solid #ff7199;border-radius: 3px;display: inline-block;float: left;margin: 10px}

.main-box3{width: 1116px;height: 270px; margin: 0 auto; overflow: hidden;position: relative;background: #ffc700}
.main-box3 .sp-box {width: 1050px;height: 240px;background: #fed62d;border: 5px solid #ffde51;margin: 15px auto 0;border-radius: 10px}
.main-box3 .sp-box .name{height: 60px; padding: 5px 0; width: 100%;background: url('http://static.lvzheng.com/lvzhengpc/images/day/namebg.png')  no-repeat center;text-align: center;}
.cy-list ul li p.list-name1{width: 270px;height: 45px;line-height: 45px; padding: 2px 10px 8px 20px; margin: 10px 0 0; font-size: 30px; font-weight: bold; color: #fff600; background: url('http://static.lvzheng.com/lvzhengpc/images/day/tcbg.png')  no-repeat center;}

.main-box4{width: 1116px;height: 580px; margin: 0 auto; overflow: hidden;position: relative;background: #ffc700 url('http://static.lvzheng.com/lvzhengpc/images/day/botbg.png')  no-repeat center bottom;}
.main-box4 .box {width: 1060px;height: 450px;margin: 15px auto 0;}
.main-box4 .short-box {width: 505px;height: 450px; background: #fed62d;border: 5px solid #ffde51;border-radius: 10px;float: left;margin-right: 30px}
.main-box4 .short-box:last-child{margin-right: 0px}
.main-box4 .short-box .name{height: 60px; padding: 5px 0; width: 100%;background: url('http://static.lvzheng.com/lvzhengpc/images/day/namebg.png')  no-repeat center;text-align: center;}
.main-box4 .short-box .short-list{}
.main-box4 .short-box .short-list ul{overflow: hidden;padding: 50px 60px;}
.main-box4 .short-box .short-list ul li{width: 165px;  margin-right: 45px;float: left;  }
.main-box4 .short-box .short-list ul li:last-child{margin-right: 0px}
.main-box4 .short-box .short-list ul li .pic{position: relative;width: 165px; height: 206px;}
.main-box4 .short-box .short-list ul li p.price{position: absolute;height: 40px;line-height: 40px; width: 165px; text-align: center;font-size: 20px;color: #424749;bottom: 0px}
.main-box4 .short-box .short-list ul li p.price span{font-size: 12px;}
.main-box4 .short-box .short-list ul li a.buy{ display: block; font-size: 20px; color: #fff9e1; width: 110px;text-align: center;height: 40px;line-height: 40px; margin: 30px auto 0; border: 1px solid #ff7199; background: #f52354;border-radius: 5px}
.get-succ{display: none;text-align: center; width: 260px;height: 90px; padding: 30px;background: #f2ebff url('http://static.lvzheng.com/lvzhengpc/images/day/succbg.png')  no-repeat center top;border: 1px solid #f99a2b;border-radius: 5px;position: absolute;top: 40%;left: 50%;margin-left: -180px}
.get-succ p.name{font-size: 28px;color: #f2385b;height: 40px;line-height: 40px;padding: 5px 0;font-weight: bold;}
.get-succ p.text{font-size: 14px;color: #636060;height: 20px;line-height: 20px;}
.succ-show{display: block;}