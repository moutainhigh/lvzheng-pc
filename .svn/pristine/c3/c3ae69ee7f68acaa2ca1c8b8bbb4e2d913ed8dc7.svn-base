@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;outline: none;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}

.mid-box{width: 100%;min-height: 1500px; padding-bottom: 65px; overflow: hidden;}
.mid-box .bannerbg{width: 100%;height: 800px; background: url('/images/mid/banner.jpg')  no-repeat center;}
.mid-box .banner{width: 1000px;height: 800px;margin: 0 auto;position: relative;}
.mid-box .banner .btag{width: 260px;height: 40px;background: url('/images/mid/btag.png')  no-repeat center;position: absolute;top: 30px;right: 10px}
.mid-box .banner .bcp{width: 770px;height: 60px;background: url('/images/mid/bcp.png')  no-repeat center;position: absolute;top: 310px;left: 120px}
.mid-box .banner .bzx{width: 160px;height: 50px;background: url('/images/mid/bzx.png')  no-repeat center;position: absolute;top: 410px;left: 325px}
.mid-box .banner .bbuy{width: 160px;height: 50px;background: url('/images/mid/bbuy.png')  no-repeat center;position: absolute;top: 410px;right: 325px}
.mid-box .banner .btext{width: 560px;height: 300px;background: url('/images/mid/btext.png')  no-repeat center;position: absolute;top: 500px;left: 230px}


.mid-box .main-box{width: 100%; min-height: 1000px; margin: 20px auto 0;overflow: hidden;}
.mid-box .main-box .titlebox{width: 100%; height: 50px;line-height: 50px; background: url('/images/mid/titleline.png')  repeat-x center;margin: 20px 0}
.mid-box .main-box .titlebox p.name{font-size: 44px;color: #666;width: 200px;background: #fff; text-align: center; height: 50px;line-height: 50px;margin: 0 auto}
.mid-box .main-box .titlebox p.long{width: 500px}
.mid-box .main-box .titlebox p.name span{color: #22c358}

.mid-box .price-tab{width: 1000px;margin: 0 auto;padding: 30px 0}
.mid-box .price-tab table{width: 910px;margin: 0 auto;text-align: center;border-collapse: collapse;}
.mid-box .price-tab table th{height: 45px;background: #7fbcfb;border: 1px solid #e6e6e6;font-size: 18px;color: #fff;font-weight: normal;}
.mid-box .price-tab table td{height: 55px;border: 1px solid #e6e6e6;font-size: 18px;color: #999999}
.mid-box .price-tab table td.thprice{background: #f2f2f2;color: #000}
.mid-box .price-tab .xwprice td{background: #fffcd5;color: #000}
.mid-box .price-tab table td.tag1{background: url('/images/mid/texp.png')  no-repeat center right 5px;}
.mid-box .price-tab table td.tag2{background: url('/images/mid/exp.png')  no-repeat center right 5px;}
.mid-box .price-tab table td.tag3{background: #fffcd5 url('/images/mid/sh.png')  no-repeat center right 5px;}

.mid-box .serflow{width: 1000px;margin: 50px auto;height: 230px;background: url('/images/mid/serflow.jpg')  no-repeat center top;position: relative;}
.mid-box .serflow .zxbtn{width: 75px;text-align: center;height: 26px;line-height: 26px;border: 1px solid #1aa2e6;font-size: 14px;color: #2ab7f7;display: block;border-radius: 5px;position: absolute;}
.mid-box .serflow .zxbtn:hover{background: #1d8cca;color: #fff}
.mid-box .serflow .b1{top: 190px;left: 25px}
.mid-box .serflow .b3{top: 190px;left: 460px}

.mid-box .clbox{width: 1000px;height: 400px;margin: 50px auto;background: url('/images/mid/clpic.jpg')  no-repeat center top;position: relative;}
.mid-box .clbox .nav{height: 30px;line-height: 30px;text-align: center;position: absolute;top: 380px;left: 250px}
.mid-box .clbox .nav span{display: inline-block;padding: 0 30px;}
.mid-box .clbox .nav span.nav1{background: url('/images/mid/num1.png')  no-repeat center left;}
.mid-box .clbox .nav span.nav2{background: url('/images/mid/num2.png')  no-repeat center left;}
.mid-box .clbox .nav span.nav3{background: url('/images/mid/num3.png')  no-repeat center left;}
.mid-box .clbox .nav span.nav4{background: url('/images/mid/num4.png')  no-repeat center left;}

.eval-box{min-height: 500px;padding: 30px 0;margin: 30px 0 0;background: #f3f8fb}
.mid-box .main-box .eval-box .titlebox p.name{background: #f3f8fb}
.eval-box .eval{width: 870px;margin: 40px auto;min-height: 500px;}

.bot-box{width: 100%;height: 65px; line-height: 65px; background: #182834;display: none;}
.bot-boxxf{position: fixed;bottom: 0px;display: block;}
.bot-box .cp-fix{width: 1000px;margin: 0 auto}
.bot-box .cp-fix .cp{font-size: 18px;height: 65px;line-height: 65px;color: #fff;width: 500px;float: left;}
.bot-box .cp-fix .cp span{font-size: 30px;color: #e4393c;display: inline-block;}
.bot-box .cp-fix .btn{height: 65px;line-height: 65px;width: 500px;float: right;text-align: right;}
.bot-box .cp-fix .btn .zx{width: 140px; text-align: center; height: 65px;line-height: 65px;font-size: 18px;color: #fff;display: inline-block;background: url('/images/mid/zx_ico.png')  no-repeat center left;;}
.bot-box .cp-fix .btn .buy{width: 140px; text-align: center; height: 65px;line-height: 65px;font-size: 18px;color: #fff;background: #b73338;display: inline-block;}


/*底部礼包浮层*/
.mid-btn { width:280px; left:-280px; height:115px; z-index:99999999; position:fixed;  bottom:0px; cursor: pointer;}
.main-mid{width: 100%;position: fixed;bottom: 0;left: 0;z-index: 999999;}
.main-mid-box{width: 100%;height: 150px;background: url(/images/mid/midbg.png) repeat;}
.mid-con{width: 1200px;height: 150px;margin:0 auto;position: relative;}
.mid-close{display:block;width: 35px;height: 35px;background: url(/images/mid/close-btn.png) no-repeat center;position: absolute;right: 20px;top: 9px;}
.mid-pic{width: 590px;height: 180px;background: url(/images/mid/mid-pic.png) no-repeat center;position: absolute;bottom: 5px;left:0;}

.mid-item{width: 290px;height: 110px; padding: 25px 35px; position: absolute;right: 230px;top: 0px;background: #000}
.mid-item .item{width: 290px;margin-bottom: 15px;overflow: hidden;position: relative;}
.mid-item .item input.tel{width: 270px; padding: 0 10px; height: 40px;line-height: 40px;font-size: 14px;color: #666;border: 1px solid #a6a6a6;border-radius: 5px;background: #fff}
.mid-item .item input.code{width: 165px; padding: 0 10px; height: 40px;line-height: 40px;font-size: 14px;color: #666;border: 1px solid #a6a6a6;border-radius: 5px;background: #fff;float: left;}
.mid-item .item .getcode{display: block;width: 80px;text-align: center;height: 40px;line-height: 40px;font-size: 14px;color: #249dd3;position: absolute;left: 100px;top: 2px;}
.mid-item .item .getbtn{display: block;width: 88px;text-align: center;height: 40px;line-height: 40px;font-size: 14px;color: #000000;background: #fdde33;border-radius: 5px;float: left;margin-left: 15px}
.mid-con .ewm{position: absolute;right: 80px;top: 5px}


/*弹层*/
.submit-box{margin-top: 10%;width: 420px;min-height: 100px;padding: 40px 0}
.submit-box .submit{width: 260px;padding-left: 75px;margin: 0 auto;}
.submit-box .bico{background: url('/images/mid/c_ico.png')  no-repeat center left;}
.submit-box .bico1{background: url('/images/mid/c_ico1.png')  no-repeat center left;}
.submit-box .submit p.name{font-size: 18px;min-height: 50px;line-height: 25px; padding: 5px 0; color: #000}
.submit-box p.text{font-size: 16px; text-align: center; padding: 10px 30px; min-height: 20px;line-height: 20px;color: #333}
.submit-box a.btn{width: 120px;height: 40px;line-height: 40px;display: block;text-align: center; margin: 10px auto 0; font-size: 16px;color: #fff;background: #009DDC;border-radius: 5px}
