@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;outline: none;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}

.land-box{width: 100%;min-height: 1500px; padding-bottom: 100px; background: #f6f6f6 url('/images/sbland/banner.jpg')  no-repeat center top;overflow: hidden;}
.land-box .banner{width: 1100px;height: 510px;margin: 0 auto;position: relative;}
.land-box .banner .btag{width: 900px;height: 200px;background: url('/images/sbland/btag.png')  no-repeat center;position: absolute;top: 130px;left: 80px}
.land-box .banner .bbtn{width: 280px;height: 70px;background: url('/images/sbland/bbtn.png')  no-repeat center;position: absolute;top: 360px;left: 400px}

.land-box .main-box{width: 1100px; min-height: 1000px; margin: 0 auto}
.left-box{width: 788px;min-height: 1000px;margin-right: 12px;float: left;}
.left-box .sea-box{padding: 15px 30px; min-height: 100px; background: #fff;border: 1px solid #afd5f5;margin-bottom: 20px}
.left-box .search{padding: 10px 0;height: 50px;line-height: 50px;width: 730px;margin: 0 auto}
.left-box .search span.sea-item{ width: 510px; height: 48px;line-height: 48px; display: block; float: left; padding: 0 19px; background: #f5fcfe; border: 2px solid #0ab7f0; border-radius: 4px 0 0 4px;position: relative;}
.left-box .search span.sea-item input{width: 510px;height: 24px;line-height: 24px; padding: 12px 0; vertical-align: top;border: none;font-size: 16px;color: #666;background: #f5fcfe}
.left-box .search a.btn{width: 170px;height: 52px;text-align: center;line-height: 52px;display: block;float: left;background: #0ab7f0;font-size: 18px;color: #fff;border-radius: 0 4px 4px 0;}
.left-box p.result{font-size: 14px;color: #222;height: 20px;line-height: 20px;padding: 5px 0}
.left-box p.result span{color: #f70f0f}

.left-box .cp-box{width: 788px;min-height: 200px;}
.left-box .cp-box .sbox{min-height: 180px;background: #fff;margin-bottom: 20px;}
.left-box .cp-box .sbox .namebox{padding: 10px 0;min-height: 40px;border-bottom: 1px solid #e5e5e5}
.left-box .cp-box .sbox p.name{font-size: 24px;color: #000;font-weight: bold;height: 40px;line-height: 40px;padding-left: 30px;border-left: 9px solid #0ab7f0}
.left-box .cp-box .sbox .sb-box{padding: 35px 20px;min-height: 330px;}
.left-box .cp-box .sbox .sb-box .list{width: 230px;height: 335px;float: left;margin: 0 9px;background: #f7f7f7;border-radius: 5px;position: relative;}

.left-box .cp-box .sbox .sb-box .list .title{font-size: 18px; font-weight: bold; color: #fff;height: 50px;line-height: 50px; width: 230px;text-align: center; border-radius: 4px 4px 0 0;}
.left-box .cp-box .sbox .sb-box .list .t1{background: #7bb2c3 url('/images/sbland/titlebg1.png')  no-repeat center bottom;}
.left-box .cp-box .sbox .sb-box .list .t2{background: #eeb621 url('/images/sbland/titlebg2.png')  no-repeat center bottom;}
.left-box .cp-box .sbox .sb-box .list .t3{background: #0eb090 url('/images/sbland/titlebg3.png')  no-repeat center bottom;position: relative;}
.left-box .cp-box .sbox .sb-box .list .t3 span.tag{display: block;position: absolute;right: 0;top: 0;width: 52px;height: 52px;background: url('/images/sbland/tj_ico.png')  no-repeat center}

.left-box .cp-box .sbox .sb-box .list .text{font-size: 14px;line-height: 22px;text-align: center; width: 230px; position: absolute;top: 65px}
.left-box .cp-box .sbox .sb-box .list .text span{color: #f40a0a}
.left-box .cp-box .sbox .sb-box .list .db{font-size: 14px;color: #999; width: 230px;text-align: center;position: absolute;top: 140px}
.left-box .cp-box .sbox .sb-box .list .db a.show{color: #178bdd;text-decoration: underline;}
.left-box .cp-box .sbox .sb-box .list .db span{color: #178bdd;}
.left-box .cp-box .sbox .sb-box .list .price{font-size: 30px;color: #f40a0a;font-weight: bold; width: 230px;text-align: center;position: absolute;top: 170px}
.left-box .cp-box .sbox .sb-box .list .price span{font-size: 14px}
.left-box .cp-box .sbox .sb-box .list .price-info{font-size: 14px;color: #666; width: 230px;text-align: center;position: absolute;top: 210px}
.left-box .cp-box .sbox .sb-box .list a.zxbtn{width: 100px; text-align: center; height: 30px; line-height: 30px; border: 1px solid #cccccc;display: block;border-radius: 5px;font-size: 14px;color: #000;background: #fff;position: absolute;top: 245px;left: 63px}
.left-box .cp-box .sbox .sb-box .list a.zxbtn:hover{background: #0ab7f0;color: #fff}
.left-box .cp-box .sbox .sb-box .list a.detail{display: block; font-size: 14px;color: #999; width: 65px; height: 20px;border-bottom: 1px dashed #999; text-align: center;position: absolute;top: 285px;left: 80px}
.left-box .cp-box .sbox .sb-box .list a.detail:hover{color: #178bdd;border-bottom: 1px dashed #178bdd}

.left-box .cp-box .sbox .sb-box .show-box{position: absolute; top:415px; left: 190px; display: none; z-index: 9999; width: 470px;min-height: 150px;padding: 8px 13px; border: 1px solid #ecbc88; border-radius: 4px;background: #fffdef;font-size: 14px;color: #666;}
.left-box .cp-box .sbox .sb-box .show-box1{position: absolute; top:560px; left: -15px; display: none; z-index: 9999; width: 290px;min-height: 150px;padding: 8px 13px; border: 1px solid #ecbc88; border-radius: 4px;background: #fffdef;font-size: 14px;color: #666;}
.left-box .cp-box .sbox .sb-box .show-box2{position: absolute; top:560px; left: 230px; display: none; z-index: 9999; width: 290px;min-height: 150px;padding: 8px 13px; border: 1px solid #ecbc88; border-radius: 4px;background: #fffdef;font-size: 14px;color: #666;}
.left-box .cp-box .sbox .sb-box .show-box3{position: absolute; top:560px; left: 480px; display: none; z-index: 9999; width: 290px;min-height: 150px;padding: 8px 13px; border: 1px solid #ecbc88; border-radius: 4px;background: #fffdef;font-size: 14px;color: #666;}

.left-box .cp-box .sbox .sb-box .show-arr{display: block; width: 15px;height: 7px;background: url('/images/sbland/ts_arr.png')  no-repeat center;position: absolute;top: -7px;left: 50%;margin-left:-7px;}
.left-box .cp-box .sbox .sb-box .sdname{font-size: 14px;color: #000;height: 20px;line-height: 20px;text-align: center;padding: 5px 0;background: url('/images/sbland/sdnamebg.png')  no-repeat center;}
.left-box .cp-box .sbox .sb-box .sdtext{overflow: hidden;}
.left-box .cp-box .sbox .sb-box .sdtext li{min-height: 20px;line-height: 20px;font-size: 14px;color: #666; margin: 6px 0;  padding-left: 15px;background: url('/images/sbland/dot.png')  no-repeat left top;}


.left-box .cp-box .sbox .apply-box{width: 738px; min-height: 200px; padding: 30px;}
.left-box .cp-box .sbox .apply-box .apply-pic{width: 738px;padding: 15px 0;}
.left-box .cp-box .sbox .apply-box .flow-xw{width: 738px;height: 45px;line-height: 45px; margin: 20px 0; background: #f5f5f5;font-size: 20px;text-align: center;}
.left-box .cp-box .sbox .apply-box .flow-xw span.xw{color: #000}
.left-box .cp-box .sbox .apply-box .flow-xw span.xw-time{color: #fc0d1b}
.left-box .cp-box .sbox .apply-box .flow-xw span.other{color: #999999}
.left-box .cp-box .sbox .apply-box .flow-xw span.other-time{color: #999999;text-decoration: line-through;}

.left-box .cp-box .sbox .adv-box{width: 758px; min-height: 200px; padding: 25px 15px;overflow: hidden;}
.left-box .cp-box .sbox .adv-box .list{width: 210px;height: 200px;float: left;margin: 10px; padding: 10px; border: 1px solid #eee;border-radius: 5px }
.left-box .cp-box .sbox .adv-box .list:hover{border: 1px solid #0AB7F0}
.left-box .cp-box .sbox .adv-box .list .aname{font-size: 16px;color: #000;height: 20px;line-height: 20px;text-align: center;font-weight: bold;padding: 80px 0 10px;}
.left-box .cp-box .sbox .adv-box .list .atext{font-size: 14px;color: #999; line-height: 20px;text-align: left;}
.left-box .cp-box .sbox .adv-box .list .ico1{background: url('/images/sbland/adv_ico1.png')  no-repeat center top;}
.left-box .cp-box .sbox .adv-box .list .ico2{background: url('/images/sbland/adv_ico2.png')  no-repeat center top;}
.left-box .cp-box .sbox .adv-box .list .ico3{background: url('/images/sbland/adv_ico3.png')  no-repeat center top;}
.left-box .cp-box .sbox .adv-box .list .ico4{background: url('/images/sbland/adv_ico4.png')  no-repeat center top;}
.left-box .cp-box .sbox .adv-box .list .ico5{background: url('/images/sbland/adv_ico5.png')  no-repeat center top;}
.left-box .cp-box .sbox .adv-box .list .ico6{background: url('/images/sbland/adv_ico6.png')  no-repeat center top;}


.land-box .main-box .right-box{width: 300px; min-height: 1000px;float: left;}
.right-box .zx-box{width: 300px;background: #fff;margin-bottom: 15px;}
.right-box .zx-box .titlebox{height: 35px;line-height: 35px;border-bottom: 1px solid #e5e5e5}
.right-box .zx-box .titlebox .title{font-size: 16px; display: inline-block; margin-left: 15px; color: #000;padding: 0 5px;border-bottom: 1px solid #1aa0e3}
.right-box .zx-box .conbox{padding: 5px 15px;}
.right-box .zx-box .conbox .text{font-size: 14px;color: #000;line-height: 20px;padding: 5px 0}
.right-box .zx-box .conbox .w-text{padding: 8px 0}
.right-box .zx-box .conbox .w-text input{width: 238px;padding: 0 10px;height: 35px;line-height: 35px;font-size: 14px;color: #000;border: 1px solid #dddddd;background: #f7f7f7;border-radius: 5px}
.right-box .zx-box .conbox a.tbtn{display: block;width: 260px; text-align: center; height: 40px;line-height: 40px;font-size: 18px;color: #fff;background: #0ab7f0;margin: 5px 0 10px}
.right-box .tel-box {height: 50px;line-height: 50px; text-align: center; background: #fff;margin-bottom: 15px;}
.right-box .tel-box .tel{font-size: 18px; display: inline-block; color: #222;padding-left: 35px;background: url('/images/sbland/tel_ico.png')  no-repeat center left;}
.right-box .tel-box .tel span{color: #ed363a}
.right-fixed{position: fixed;top: 0px}

.conbox .hot-tab {width: 270px; height: 25px; border-bottom: 1px solid #0ab7f0;margin-top: 10px}
.conbox .hot-tab li a {display:block; height: 25px;line-height: 25px; width:75px;text-align:center;font-size:14px; color: #666; float: left; cursor:pointer;}
.conbox .hot-tab li.current a {background:#0ab7f0;color:#fff;font-weight: 800;border-radius: 3px}

.conbox .hot-con{padding: 5px 0;min-height: 200px;width: 270px; }
.conbox .hot-con .sublist .list .hname{font-size: 14px;color: #000;min-height: 20px;line-height: 20px; margin: 8px 0; cursor: pointer; padding: 0 0 0 15px;background: url('/images/sbland/dot.png')  no-repeat left top;}
.conbox .hot-con .sublist .list:hover .hname{color: #178bdd}
.conbox .hot-con .sublist .list .ans{padding: 3px 8px; width: 250px; border: 1px solid #ecbc88; position: relative; border-radius: 4px;display: none;background: #fffdef;font-size: 14px;color: #666;}
.conbox .hot-con .sublist .list .ts-arr{display: block; width: 15px;height: 7px;background: url('/images/sbland/ts_arr.png')  no-repeat center;position: absolute;top: -7px;left: 30px}


.bot-box{width: 100%;height: 40px; line-height: 40px; background: #182834;padding: 10px 0;display: none;}
.bot-boxxf{position: fixed;bottom: 0px;display: block;}
.bot-box .tel-box{width: 800px;margin: 0 auto}
.bot-box .tel-box .tel{font-size: 24px;color: #ec373a;padding-left: 30px;background: url('/images/sbland/tel_ico.png')  no-repeat center left;float: left;}
.bot-box .tel-box .item{float: left; width: 440px; margin-left: 30px}
.bot-box .tel-box .item input.w-text{width: 270px;height: 40px; line-height: 40px; background: #fff;padding: 0 10px;border: none;float: left;font-size: 16px;color: #333}
.bot-box .tel-box .item a.btn{width: 150px;text-align: center;height: 40px;line-height: 40px;font-size: 20px;color: #fff;background: #ec373a;display: block;float: left;}


/*左侧描点 浮动导航*/
.splist-nav{width: 60px;min-height: 55px; position: absolute; top: 20px; left: -100px; }
.splist-nav .spnav{width: 60px;margin: 0 auto;}
.splist-nav .spnav ul{}
.splist-nav .spnav ul li a{width: 60px;height: 40px; text-align: center; margin: 10px; line-height: 20px;padding: 10px 0; font-size: 12px;color: #222; display: block;background: #fff;border-radius: 50%}
.splist-nav .spnav ul li a:hover{cursor: pointer;color: #fff;background: #0ab7f0}
.splist-nav .spnav ul li a.cur{background: #0ab7f0; color: #fff; width: 70px;height: 50px;  line-height: 25px;padding: 10px 0; margin: 5px;  font-size: 14px; display: block;border-radius: 50%}
.splist-navxf { position: fixed; top: 20px; left: 50%; margin-left: -650px; z-index: 9000; display: block;}
.main-splist{position: relative;}

/*查询弹层*/
#sb-search{margin-top: 10%;width: 520px;min-height: 200px;background: #fdfbe9}
#sb-search .sbtitle{height: 65px;line-height: 65px; padding: 0 20px; font-size: 22px;color: #fff;background: #7790ab url('/images/sbland/ctitlebg.png')  no-repeat center bottom;border-radius: 5px 5px 0 0 }
#sb-search .sbtitle span{font-size: 14px;}
#sb-search .submit{padding: 10px 0}
#sb-search .submit p.name{font-size: 14px;height: 30px;line-height: 30px;color: #666;text-align: center;}
#sb-search .submit p.name span{color: #f81212}
#sb-search .submit .item {width: 350px;margin: 10px auto}
#sb-search .submit .item input.tel{width: 328px; padding: 0 10px; height: 38px;line-height: 38px;font-size: 14px;color: #999;border: 1px solid #e1e1e1;border-radius: 5px;}
#sb-search .submit a.seabtn{width: 350px;text-align: center;height: 50px;line-height: 50px;background: #0ab7f0;display: block;margin: 10px auto;font-size: 18px;color: #fff;border-radius: 5px}

/*提交成功弹层*/
#submit-box{margin-top: 10%;width: 420px;min-height: 100px;padding: 50px 0}
#submit-box .submit{width: 260px;padding-left: 75px;margin: 0 auto;background: url('/images/sbland/c_ico.png')  no-repeat center left;}
#submit-box .submit p.name{font-size: 18px;height: 40px;line-height: 40px;color: #000}
#submit-box .submit p.text{font-size: 14px;min-height: 20px;line-height: 20px;color: #666}

.land-btn { width:200px; left:-200px; height:145px; z-index:9999; position:fixed;  bottom:0px; cursor: pointer;}
.land-hb{width: 100%;position: fixed;bottom: 0;left: 0;z-index: 999999;}
.land-hb-box{width: 100%;height: 150px;background: url(/images/sbland/cbotbg.png) repeat;}
.land-hb .hb-con{width: 1260px;height: 150px;margin:0 auto;position: relative;}
.land-hb .hb-close{display:block;width: 35px;height: 35px;background: url(/images/sbland/close_btn.png) no-repeat center;position: absolute;right: 20px;top: 9px;}
.land-hb .hb-pic{width: 1260px;height: 170px;background: url(/images/sbland/bot_pic.png) no-repeat center;position: absolute;bottom: 0;left:0;}

.land-hb .hb-item{width: 370px;height: 100px; padding: 30px 40px 20px; background: #000; position: absolute;right: 70px;top: 0px}
.land-hb .hb-item .item{width: 370px;margin-bottom: 10px;overflow: hidden;}
.land-hb .hb-item .item label{height: 40px; line-height: 40px; width: 75px;text-align: right;font-size: 14px;color: #fff;display: block;float: left;}
.land-hb .hb-item .item input.tel{width: 268px; padding: 0 10px; height: 38px;line-height: 38px;font-size: 14px;color: #999;border: 1px solid #a6a6a6;border-radius: 5px;}
.land-hb .hb-item .item input.code{width: 168px; padding: 0 10px; height: 38px;line-height: 38px;font-size: 14px;color: #999;border: 1px solid #a6a6a6;border-radius: 5px;;float: left;}
.land-hb .hb-item .item .getcode{display: block;width: 85px;text-align: center;height: 40px;line-height: 40px;font-size: 14px;color: #000;background: #fdde33;border-radius: 5px;float: left;margin-left: 15px}
