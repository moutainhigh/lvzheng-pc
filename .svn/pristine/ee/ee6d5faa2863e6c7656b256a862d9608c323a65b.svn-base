@charset "utf-8";
*{padding:0;margin:0;}
body{padding:0;margin:0;font-size:12px;font-family:"微软雅黑";color: #333;background:#fff;}
img{border:none;}
a{text-decoration:none;cursor:pointer;color:#666;}
a:hover,a:hover {text-decoration:none;color:#55acee;}
input{color: #777;}
ul{padding:0;margin:0;list-style:none;}
li,p{padding:0;margin:0;}
.clear{clear:both;}
::-webkit-input-placeholder { /* WebKit browsers */color: #ccc;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color: #ccc;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #ccc;}
:-ms-input-placeholder { /* Internet Explorer 10+ */color: #ccc;}

.bot-main{padding-bottom: 70px}
.down-box{min-height: 1000px;width: 100%; background: #ecf9ff;}
.down-box .banner{height: 500px;width: 100%; background: #1fb8ff;}
.down-box .ban-box{height: 500px;width: 1050px;margin: 0 auto; position: relative;}
.down-box .banner .tag{width: 650px;height: 450px;position: absolute;top: 48px;left: 190px}
.down-box .banner .btext{width: 570px;padding: 12px 0 17px;height: 30px;line-height: 30px;font-size: 24px; font-weight: bold; color: #11a0e3; text-align: center; background: url('../images/btext.png')  no-repeat center;position: absolute;top: 380px;left: 270px}

.main-box{width: 1050px;margin: 0 auto;min-height: 400px;padding: 40px 0;overflow: hidden;}
.main-box .pic-box{width: 540px;min-height: 400px;margin-right: 30px;float: left;}
.main-box .pic-box img{width: 540px;}
.main-box .pic-box .imgtext{font-size: 30px;height: 40px;line-height: 40px; padding: 5px 0; color: #666;text-align: center;background: url('../images/imgtext.png')  no-repeat center;}

.main-box .con-box{width: 480px;float: left;min-height: 400px}
.main-box .con-box .list{padding: 0 0 20px;min-height: 50px;}
.main-box .con-box .list p.name-box{height: 30px; line-height: 30px;padding: 10px 0;border-bottom: 2px solid #d8e8ef}
.main-box .con-box .list p.name-box span.blue{display: inline-block;height: 30px;line-height: 30px;font-size: 30px; color: #1fb8ff;padding-left: 20px; margin-left: 30px; border-left: 5px solid #1fb8ff}
.main-box .con-box .list p.name-box span.yellow{display: inline-block;height: 30px;line-height: 30px;font-size: 30px; color: #ff8a00;padding-left: 20px; margin-left: 30px; border-left: 5px solid #ff8a00}
.main-box .con-box .list p.name-box span.red{display: inline-block;height: 30px;line-height: 30px;font-size: 30px; color: #f83b6c;padding-left: 20px; margin-left: 30px; border-left: 5px solid #f83b6c}
.main-box .con-box .list p.name-box span.green{display: inline-block;height: 30px;line-height: 30px;font-size: 30px; color: #22d694;padding-left: 20px; margin-left: 30px; border-left: 5px solid #22d694}
.main-box .con-box ul.ul-list{overflow: hidden;min-height: 50px;padding: 10px 0}
.main-box .con-box ul.ul-list li{height: 30px;line-height: 30px;font-size: 20px;padding: 10px 0 10px 30px;cursor: pointer;}
.main-box .con-box ul.ul-list li a.link{display: block;}
.main-box .con-box ul.ul-list li p.text{font-size: 20px;width: 340px;float: left;}
.main-box .con-box ul.ul-list li p.text span{font-size: 14px}
.main-box .con-box ul.ul-list li p.down-btn{display: inline-block;float: right;color: #666;width: 60px; padding: 0 15px 0 30px; text-align: center;height: 28px;line-height: 28px;border: 1px solid #7d7d7d;border-radius: 5px;background: url('../images/down_ico.png')  no-repeat center left;}
.main-box .con-box ul.ul-list li:hover p.text{color: #1fb8ff}
.main-box .con-box ul.ul-list li:hover p.down-btn{color: #1fb8ff;background: url('../images/down_hover.png')  no-repeat center left;border: 1px solid #1fb8ff;}

.buy-banner{width: 100%;height: 150px;background: #f83b6c}
.buy-banner .box{width: 1050px; height: 150px; margin: 0 auto;background: url('../images/buy_banner.jpg')  no-repeat center;position: relative;}
.buy-banner .box a.buy-btn{width: 160px; text-align: center; height: 55px; line-height: 55px; display: block;font-size: 24px;color: #fff;background: #f8c03b;border-radius: 6px;position: absolute;top: 50px;right: 0px}

.lawyer{width: 1055px; margin: 0 auto; padding: 40px 0; min-height: 410px;}
.lawyer p.name{height: 30px;border-bottom: 1px solid #1fb8ff;position: relative;}
.lawyer p.name span{font-size: 30px;line-height: 30px;width: 220px; text-align: center;position: absolute;top: 15px; left: 50%; margin-left: -110px; background: #ecf9ff;color: #1fb8ff}
.lawyer .box{min-height: 410px;padding: 70px 0}
.lawyer .box .pic{width: 280px;height: 410px;float: left;margin-right: 70px;}
.lawyer .box .text-box{width: 700px;height: 310px;float: left;padding-top: 100px}
.lawyer .box .text-box p.blurb{font-size: 30px;height: 30px;line-height: 30px; margin-bottom: 30px; color: #333333;padding-left: 10px;border-left: 5px solid #333333}
.lawyer .box .text-box p.btext{font-size: 24px;color: #333;line-height: 40px;}
.lawyer .box .text-box p.btext span{color: #dc1312}

.float-box{width: 100%;height: 70px;line-height: 70px;background: #182933;display: none;}
.float-box a.btn{width: 430px;font-size: 30px;color: #fff;text-align: center;margin: 0 auto;display: block;background: #dc1312}
.float-boxxf {position: fixed;bottom: 0px;z-index: 9000;display: block;}

.img-replace {display: inline-block;overflow: hidden;text-indent: 100%;color: transparent;white-space: nowrap;}
.cd-popup {position: fixed;left: 0;top: 0;height: 100%;width: 100%;z-index: 99999;opacity: 0;visibility: hidden;-webkit-transition: opacity 0.3s 0s, visibility 0s 0.3s;-moz-transition: opacity 0.3s 0s, visibility 0s 0.3s;transition: opacity 0.3s 0s, visibility 0s 0.3s;}
.cd-popup.is-visible {opacity: 1;visibility: visible;-webkit-transition: opacity 0.3s 0s, visibility 0s 0s;-moz-transition: opacity 0.3s 0s, visibility 0s 0s;transition: opacity 0.3s 0s, visibility 0s 0s;}
.cd-popup-container {position: relative;width: 90%;max-width: 735px; height: 100%; margin: 0 auto 0;  border-radius: 8px;box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);-webkit-transform: translateY(-40px);-moz-transform: translateY(-40px);-ms-transform: translateY(-40px);-o-transform: translateY(-40px);transform: translateY(-40px);-webkit-backface-visibility: hidden;-webkit-transition-property: -webkit-transform;-moz-transition-property: -moz-transform;transition-property: transform;-webkit-transition-duration: 0.3s;-moz-transition-duration: 0.3s;transition-duration: 0.3s;}
.is-visible .cd-popup-container {-webkit-transform: translateY(0);-moz-transform: translateY(0);-ms-transform: translateY(0);-o-transform: translateY(0);transform: translateY(0);}

/*弹出层*/
.cdown-box{width: 440px;height: 250px; padding: 19px 38px; background: #fff;border: 1px solid #c9d4d9; margin-top: 100px;}
.getdown-box p.down-name{font-size: 24px;color: #666;padding: 30px 0;text-align: center;line-height: 40px}
.getdown-box p.btn{text-align: center;}
.getdown-box p.btn a.qx{display: inline-block; height: 50px;line-height: 50px;width: 170px; text-align: center;font-size: 18px;color: #333; border: 1px solid #b3b3b3;margin-right: 10px}
.getdown-box p.btn a.qd{display: inline-block; height: 50px;line-height: 50px;width: 170px; text-align: center;font-size: 18px;color: #fff; border: 1px solid #f8943b;background: #f8943b}
.getdown-box .share{height: 20px;line-height: 20px;padding: 20px 0;}
.getdown-box .share ul{overflow: hidden;}
.getdown-box .share ul li.ck-off{float: left;font-size: 14px;color: #666;min-width: 80px;margin: 0 10px; cursor: pointer; padding-left: 25px;background: url('../images/check_off.png')  no-repeat center left;}
.getdown-box .share ul li.ck-on{float: left;font-size: 14px;color: #666;min-width: 80px;margin: 0 10px; cursor: pointer; padding-left: 25px;background: url('../images/check_on.png')  no-repeat center left;}
.wx-ewm{position: absolute;top: -1px;left: -2px;display: none;}
.wx-ewm .close-btn{width: 30px;height: 30px;background: url('../images/close_btn.png')  no-repeat center;position: absolute;right: 20px;top: 20px;}