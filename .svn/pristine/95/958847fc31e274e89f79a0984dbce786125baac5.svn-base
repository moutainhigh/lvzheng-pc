<script>



$(".gofetch").click(function(){
	var packetId = $(this).attr("data-pid");
	var thisE = $(this);
	$.ajax({
		url:"/preferential/fetch",
		data:{"packetId":packetId},
		type:"post",
		dataType:"json",
		complete:function(data){
			if(data.readyState == 4 && data.status == 200){
				var responeDataJSON = $.parseJSON(data.responseText);
				if(responeDataJSON.success){
					thisE.removeClass("gofetch");
					//换样式
					thisE.children().removeClass("on").addClass("geton");
					thisE.find(".get").text("已领取");
					$(".get-succ").addClass("succ-show")
			        setTimeout(function(){
			            $(".get-succ").removeClass("succ-show");
			        },2000);
				}else{
					if(!!responeDataJSON.login){
						window.location = "/login.html?test=1&path="+window.location.href+"?packetId="+packetId;
					}
					if(!!responeDataJSON.msg){
						alert(responeDataJSON.msg);
					}
				}
			}
		}
	})
});


$(function(){
	var J_packetId = $("#J_packetId").val();
	if(J_packetId){
		$.ajax({
    		url:"/preferential/fetch",
    		data:{"packetId":J_packetId},
    		type:"post",
    		dataType:"json",
    		complete:function(data){
    			if(data.readyState == 4 && data.status == 200){
					var responeDataJSON = $.parseJSON(data.responseText);
					if(responeDataJSON.success){
						window.location = "http://www.lvzheng.com/preferential/active";
					}else{
						if(!!responeDataJSON.login){
							window.location = (responeDataJSON.login);
						}
						if(!!responeDataJSON.msg){
							alert(responeDataJSON.msg);
						}
					}
    			}
    		}
    	})
	}
})

