    });
    $('.hb-btn').click(function(){ 
        $('.main-hb').animate({'left':'0'},400);
        $('.hb-btn').animate({'left':'-200px'},400);
    })

    
    //判断是不是要隐藏
    popgift(); */
    
    
    // 底部礼包浮层
    $(".mid-close").click(function(){      
        $(".main-mid").animate({"left":-$(".main-mid").width()},300);
        $('.mid-btn').animate({'left':'0px'},400);
    });
    $('.mid-btn').click(function(){ 
        $('.main-mid').animate({'left':'0'},400);
        $('.mid-btn').animate({'left':'-280px'},400);
    })
    
    
    //banner改版js
    $(".banner-box").yx_rotaion({auto:true});
   
});


</script>

